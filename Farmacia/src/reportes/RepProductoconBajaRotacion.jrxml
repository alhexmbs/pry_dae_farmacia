	<parameter name="limite" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    p.id_producto,
    p.nombre,
    COALESCE(SUM(dpp.cantidad), 0) AS unidades_vendidas  
FROM 
    producto_farmaceutico p
LEFT JOIN 
    PEDIDO_DETALLE_PRODUCTO_FORMA dpp ON p.id_producto = dpp.id_producto  
LEFT JOIN 
    pedido pe ON dpp.id_pedido = pe.id_pedido  
WHERE 
    pe.fecha_hora > CURRENT_DATE - INTERVAL '30 days'  
GROUP BY 
    p.id_producto, p.nombre  
ORDER BY 
    unidades_vendidas DESC  
LIMIT $P{limite};]]>
	</queryString>
	<field name="id_producto" class="java.lang.Integer"/>
	<field name="nombre" class="java.lang.String"/>
	<field name="unidades_vendidas" class="java.lang.Long"/>
