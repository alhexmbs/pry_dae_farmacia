	<queryString>
		<![CDATA[SELECT 
    pf.nombre AS producto,
    COALESCE(SUM(pdp.cantidad), 0) AS unidades_vendidas,
    dpf.stock AS stock_actual
FROM 
    producto_farmaceutico pf
LEFT JOIN 
    detalle_producto_forma dpf ON pf.id_producto = dpf.id_producto
LEFT JOIN 
    pedido_detalle_producto_forma pdp ON dpf.id_producto = pdp.id_producto
LEFT JOIN 
    pedido p ON pdp.id_pedido = p.id_pedido
WHERE 
    p.fecha_hora BETWEEN '2024-01-01' AND '2024-12-31'  
GROUP BY 
    pf.nombre, dpf.stock
HAVING 
    SUM(pdp.cantidad) <= 10 
ORDER BY 
    unidades_vendidas ASC;
]]>
	</queryString>
	<field name="producto" class="java.lang.String"/>
	<field name="unidades_vendidas" class="java.lang.Long"/>
	<field name="stock_actual" class="java.lang.Integer"/>
